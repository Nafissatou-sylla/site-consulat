
{% extends './home/base.html.twig' %}

  {% block titre %}{% endblock %}
  {% block css %}{% endblock %}
{%block body%}    
<main> 
    <div class="special-container m-5">

        <h2>Services aux Sénégalais</h2>
        <div class="card-deck">
          <div class="card">
          <img class="card-img-top" src="https://th.bing.com/th/id/OIP.KIIYMCLscIa5nGbIjqm3zgAAAA?w=233&h=180&c=7&r=0&o=5&dpr=1.5&pid=1.7" alt="Documents de Voyage">
          <div class="card-body">
           <h5 class="card-title">Documents de Voyage</h5>
           <p class="card-text">Première demande de passeport, Renouvellement de passeport, Sauf conduit</p>
           <a class="goto-link" href="{{ path('app_documentsdevoyage') }}">Consulter-></a>
         </div>
       </div>
       <div class="card">
         <img class="card-img-top" src="https://www.musicinafrica.net/sites/default/files/images/article/201412/visa-senegal.jpg" alt="Etat-Civil">
         <div class="card-body">
           <h5 class="card-title">Etat-Civil</h5>
           <p class="card-text">Carte consulaire, Certificat de coutume, Transcription de naissance, Laissez-passer mortuaire...</p>
           <a class="goto-link" href="etat-civil" title="Etat-Civi">Consulter-></a>
        </div>
       </div>
       <div class="card">
         <img class="card-img-top" src="https://senegalembassy.co.za/wp-content/uploads/2019/01/carte_biometrique_0_0.jpg" alt="CNI">
         <div class="card-body">
           <h5 class="card-title">Carte d'Identité et Immatriculation</h5>
           <p class="card-text">Demande d'immatriculation, Demande ou Retrait de Carte d'Identité</p>
           <a class="goto-link" href="{{ path('app_cni') }}">Consulter-></a>

        </div>
       </div>
       <div class="card">
         <div class="card-body">
           <h5 class="card-title">Autres Services</h5>
           <p class="card-text">Service juridique, Légalisations, </p>
           <a class="goto-link" href="autres-services" title="Autres Services">Consulter-></a>
     
         </div>
       </div>
     
     </div>
     
     <!-- ---------------------------------------------------- -->
     
     <div class="separator"></div>
         
     <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
     <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
     <script src="https://kit.fontawesome.com/14273d579a.js" crossorigin="anonymous"></script>
     <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
