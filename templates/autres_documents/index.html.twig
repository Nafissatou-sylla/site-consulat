{% extends 'base.html.twig' %}

{%block titre%} Autres Documents {%endblock%}
{%block css%}{%endblock%}

{%block body%}
    <div class="container" id="page">
       
       <div class="special-container m-1">
         <div class="container-fluid small guide-consulaire "><br><br>
            <div class="breadcrumbs container" style="text-align:left">
                <div class="row">
                    <div class="col-10">
                        <ul class="list" style="color:grey">
                            <li class="list-inline-item">
                              <a class="goto-link" href="/">Accueil ></a>
                                <svg class="icon list__icon" width="12" height="12">
                                  <use xlink:href="#icon-arrow-down"></use>
                                </svg>
                            </li>
                            <li class="list-inline-item">
                              <a href="{{path('app_servicesauxsenegalais') }}">Services aux Sénégalais ></a>
                                <svg class="icon list__icon" width="12" height="12">
                                  <use xlink:href="#icon-arrow-down"></use>
                                </svg>
                            </li>
                            <li class="list-inline-item">
                               Autres services
                             </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-inner">
            <div class="main">
                <div class="main-inner group">
                    <main class="content" id="content">
                       <div class="section-header">
                         <br><h2>Autres documents</h2>
                       </div>
                        <div class="hu-pad group">     
                        <article class="group post-35 page type-page status-publish hentry">    
                            <div class="entry themeform">                           
                                <br><h4><strong>Certificat de Concordance</strong></h4>                             
                                <ul>
                                    <li>Documents sur lesquels la concordance est sollicitée;</li>
                                    <li>Un extrait de naissance sénégalais ;</li>                                                                   
                                </ul>

                                <br><h4><strong> Attestation de nationalité </h4></strong>
                                <ul>
                                    <li> Un extrait de naissance du mineur transcrit au Consulat <strong>+</strong> pièce d'identité
                                    du père </li> 
                                </ul>

                                <br><h4><strong>Etat de famille </h4></strong>
                                <ul>
                                    <li>Livret de famille <strong>+</strong> CNI des 2 époux <strong>+</strong>
                                        Un extrait pour chaque enfant;
                                    </li>
                                    <li>Un certificat de vie collectif.</li>
                                </ul>

                                <br><h4><strong> Certificat de vie</h4></strong>
                                <ul>
                                    <li>Pièce d'identité du demandeur <br> <strong>Présence obligatoire</strong> </li>
                                </ul>

                                <br><h4><strong>Attestation PACS </h4></strong>
                                <ul>
                                    <li>Pièce d'identité sénégalaise(CNI ou Passeport). </li>
                                </ul>

                            <nav class="pagination group">
                                    </nav><!--/.pagination-->
                            <div class="clear"></div>
                            </div><!--/.entry-->
                            </article>
                      </div><!--/.hu-pad-->
                    </main><!--/.content-->
                </div><!--/.main-inner-->
            </div><!--/.main-->
        </div><!--/.container-inner-->
    </div><!--/.container-->

{%block scripts%}{%endblock%}
{%endblock%}
