{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }} 
    <div class="container">
                <div class="row">
                    <div class="col-4">
                        {{form_row(form.prenom)}}
                    </div>
                    <div class="col-4">
                        {{form_row(form.nom)}}
                    </div>
                </div>


                <div class="row">
                    <div class="col-4">
                        {{form_row(form.email)}}
                    </div>
                    <div class="col-4">
                        {{form_row(form.password)}}
                    </div>
                  
                </div>

                
                
                   
                <div class=col-6>
                    {{form_rest(form)}} 
                </div><br><br>
                <button class="btn btn" style="background-color:#009688">{{ button_label|default('Enregistrer') }}</button>

    </div>
{{ form_end(form) }}
             